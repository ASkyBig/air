{
    "for循环": {
        "prefix": "for",
        "body": [
            "for (const ${2:item} of ${1:array}) {",
            "\t$3",
            "}"
        ],
        "description": "for循环"
    },
    "for 普通循环": {
        "prefix": "for",
        "body": [
            "for (let ${1:i} = 0; ${1: i} < ${2:arr}.lenght; ${1:i}++) {",
            "  const ${3:item} = ${2:arr}[${1:i}]",
            "}"
        ]
    },
    "ajax": {
        "prefix": "ajax",
        "body": [
            "$.ajax({",
            "    url: '$1',",
            "    method: '${2:POST}',",
            "    datatype: 'json',",
            "    success: data => {",
            "        $3;",
            "    },",
            "    error: err => {",
            "        $4;",
            "    }",
            "})"
        ],
        "description": "ajax模块"
    },
    "antd <Select>": {
        "prefix": "select",
        "body": [
           "<Select",
           "  defaultValue={$1}",
           "  onChange={${2:value => {}}}",
           ">",
           "  {",
           "    ${3:optionArr}.map(item => {",
           "      return <Option value={item.value}>{item.label}</Option>",
           "    })",
           "  }",
           "</Select>",
           "$4"
        ],
        "description": "antd <Select>"
    },
    "antd <Button>": {
      "prefix": "button",
      "body": [
        "<Button",
        "  type={${1:primary}}",
        "  onClick={${2:handleClick}}",
        ">",
        "  ${3:确定}",
        "</Button>",
        "$4"
      ],
      "description": "antd <Button>"
    },
    "antd <Tabs>": {
        "prefix": "tabs",
        "body": [
            "<Tabs defaultActiveKey={${1:key}} activeKey={${2:key}} onChange={${3:activeKey => {}}}>",
            "  <TabPane tab={${4:Tab 1}} key={${5:1}}>",
            "    Content of Tab Pane 1",
            "  </TabPane>",
            "  <TabPane tab={${4:Tab 2}} key={${5:2}}>",
            "    Content of Tab Pane 2",
            "  </TabPane>",
            "</Tabs>",
            "$6"
        ],
        "description": "antd <Tabs>"
    },
    "antd <Tag>": {
        "prefix": "tag",
        "body": [
            "<Tag closeable visible onClose={${1:e => {}}}>",
            "  {${2:tagName}}",
            "</Tag>",
            "$3"
        ],
        "description": "antd <Tag>"
    },
    "antd <Input>": {
        "prefix": "input",
        "body": [
            "<Input",
            "  placeholder=\"${1:placeholder}\"",
            "  value={${2:value}}",
            "  onChange={${3:callback}}",
            "/>",
            "$4"
        ],
        "description": "antd <Input>"
    },
    "antd <Radio>": {
        "prefix": "radio",
        "body": [
            "<Radio value={${1:value}}>",
            "  {${2:label}}",
            "</Radio>",
            "$4"
        ],
        "description": "antd <Radio>"
    },
    "antd <RadioGroup>": {
        "prefix": "radiogroup",
        "body": [
            "<Radio.Group",
            "  disabled={${1:false}}",
            "  onChange={${2:callback}}",
            ">",
            "  {",
            "    ${3:optionArr}.map(item => {",
            "      return <Radio value={${4:value}}>{${5:label}}</Radio>",
            "    })",
            "  }",
            "</Radio.Group>",
            "$6"
        ],
        "description": "antd <RadioGroup>"
    },
    "antd <Table>": {
        "prefix": "table",
        "body": [
            "<Table",
            "  dataSource={${1:dataList}}",
            "  pagination={${2:false}}",
            "  columns={${3:columns}}",
            "/>",
            "$4"
        ],
        "description": "antd <Table>"
    },
    "antd <Modal>": {
        "prefix": "modal",
        "body": [
            "<Modal",
            "  visible={${1:false}}",
            "  width=\"${2:1000px\"}",
            "  title=\"${3:title}\"",
            "  onOk={${4:callback}}",
            "  onCancel={${5:callback}}",
            ">",
            "{${6:content}}",
            "</Modal>",
            "$7"
        ],
        "description": "antd <Modal>"
    },
    "antd <Checkbox>": {
        "prefix": "checkbox",
        "body": [
            "<Checkbox",
            "  style={{ $1 }}",
            "  key={${2: key}}",
            "  checked={${3:false}}",
            "  onChange={${4:e => e.target.value}}",
            ">",
            "{${5:label}}",
            "</Checkbox",
            "$6"
        ],
        "description": "antd <Checkbox>"
    }
}